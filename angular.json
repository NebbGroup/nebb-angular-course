{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nebb-angular-course": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "nebb",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/nebb-angular-course",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": []
          },
          "configurations": {
            "nb": {
            "aot": true,
            "outputPath": "dist/nebb-angular-course-nb/",
            "i18nFile": "src/locale/messages.nb.xlf",
            "i18nFormat": "xlf",
            "i18nLocale": "nb"
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "nebb-angular-course:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "nebb-angular-course:build:production"
            },
            "staging": {
              "browserTarget": "nebb-angular-course:build:staging"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nebb-angular-course:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "nebb-angular-course-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "nebb-angular-course:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nebb-angular-course:serve:production"
            },
            "staging": {
              "devServerTarget": "nebb-angular-course:serve:staging"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "nebb-rest-service": {
      "root": "projects/nebb-rest-service",
      "sourceRoot": "projects/nebb-rest-service/src",
      "projectType": "library",
      "prefix": "nebb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/nebb-rest-service/tsconfig.lib.json",
            "project": "projects/nebb-rest-service/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/nebb-rest-service/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/nebb-rest-service/src/test.ts",
            "tsConfig": "projects/nebb-rest-service/tsconfig.spec.json",
            "karmaConfig": "projects/nebb-rest-service/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/nebb-rest-service/tsconfig.lib.json",
              "projects/nebb-rest-service/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "nebb-oee-status": {
      "root": "projects/nebb-oee-status",
      "sourceRoot": "projects/nebb-oee-status/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/nebb-oee-status/tsconfig.lib.json",
            "project": "projects/nebb-oee-status/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/nebb-oee-status/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/nebb-oee-status/src/test.ts",
            "tsConfig": "projects/nebb-oee-status/tsconfig.spec.json",
            "karmaConfig": "projects/nebb-oee-status/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/nebb-oee-status/tsconfig.lib.json",
              "projects/nebb-oee-status/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "nebb-angular-course"
}
